package cn.com.jr.HTUmidware.serverofdev.protocol.send.senddatastrategy;

public class StrToHexByteArrary implements DataToHexStrategy {
	
	private static StrToHexByteArrary strToHexByteArrary=new StrToHexByteArrary();
	
	
	private StrToHexByteArrary(){
		
	}
	
	public static StrToHexByteArrary getInstance(){
		return strToHexByteArrary;
	}
	
	
	
	
	
	
	@Override
	public byte[] toHexByteArrStrategy(String content) {
		// TODO Auto-generated method stub
		byte[] contentByteArr = content.getBytes();
		return contentByteArr;
	}
	
	
	
	
}
